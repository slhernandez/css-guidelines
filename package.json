{
  "name": "SortableImageGallery",
  "version": "0.0.1",
  "description": "Basic photo ordering tool based on the images and JavaScript included. Thumbnails of images are displayed in a grid layout and sortable by dragging images to their desired position. ",
  "dependencies": {
    "browserify": "7.0.3",
    "browserify-shim": "^3.8.1",
    "del": "^1.1.0",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-minify-css": "^0.3.11",
    "gulp-notify": "^2.1.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^1.2.4",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.0.2",
    "jquery": "^2.1.3",
    "jquery-ui": "^1.10.5",
    "lodash": "^2.4.1",
    "vinyl-source-stream": "^1.0.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "./src/site/scripts/libs/jquery-2.1.3.min.js": "$"
  },
  "scripts": {
    "start": "gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:slhernandez/SortableImageGallery.git"
  },
  "author": "Steve L. Hernandez"
}
